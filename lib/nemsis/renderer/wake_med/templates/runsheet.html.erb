
<%
   NBSP ||= "&nbsp;"
%>

<% if @fancy_html %>
    <%= HTML_STYLE %>
<% end %>

<h4>Wake County EMS System - Patient Care Record</h4>
<%= borderless_table %>
    <%= start_row %>
      <%= borderless_cell "Name:", "#{@parser.E06_01}, #{@parser.E06_02}" %>
      <%= borderless_cell "Incident #:", @parser.E02_02 %>
      <%= borderless_cell "Date:", @parser.parse_time('E05_03', true) %>
    </tr>
  </table>

<%= title = "Patient Information"
    start_table title, 6 %>
  <tbody>
  <!--Last, address, dob-->
  <%= row 'E06_01', 'E06_04', 'E06_16' %>
  <!--First, city/st/zip, weight-->
  <%= start_row %>
    <%= labeled_cell 'First', @parser.E06_02 %>
    <%= labeled_cell 'City, St Zip', "#{@parser.parse_city('E06_05')}, #{@parser.parse_state('E06_07')} #{@parser.E06_08}" %>
    <%= labeled_cell 'Weight', @parser.weight_in_words %>

  </tr>
  <!--Middle, phone, SSN-->
  <%= row 'E06_03', 'E06_17', 'E06_10' %>
  <!--Gender, county, physician -->
  <%= row 'E06_11', {'County' => "parse_county('E06_06')"}, {'Physician' => "concat('E12_04', 'E12_06')"} %>
  <!--Age, country-->
  <%= row( {'Age' => 'age_in_words'}, 'E06_09', '') %>
  <%= start_row %>
    <%= labeled_cell "Advanced Directive", @parser.E12_07, 5 %>
  </tr>
  </tbody>
</table>
<br>
<%= title = "Clinical Impression"
    start_table title, 6 %>
  <tbody>
      <!-- Primary Impression, Anatomical Position, Injury -->
      <%= row( {'Primary Impression' => "concat('E09_19')"}, 'E09_11', {'Injury' => "concat('E10_01', 'E10_02', 'E10_03', 'E10_11')"} )%>
      <!-- Secondary Impression, Chief Complaint (Dur), Medical/Trauma -->
      <%= row( {'Secondary Impression' => "concat('E09_20')"}, {'Chief Complaint' => "concat('E09_05', 'E09_06', 'E09_07')"}, {'Medical/Trauma' => "parse_value_of('MedicalTrauma')"} )%>
      <!-- Protocol Used (D04_08), Secondary Complaint (Dur), Barriers of Care -->
      <%= row 'E17_01', {'Secondary Complaint' => "concat('E09_08', 'E09_09', 'E09_10')"}, 'E12_27' %>
      <!-- Signs & Symptoms, Alcohol/Drugs -->
      <%= row({"Signs &amp; Symptoms" => "concat_with_comma('E09_13', 'E09_14', 'E09_17', 'E09_18')"}, 'E12_19', 3) %>
  </tbody>
</table>
<br>
<% if @parser.has_content('E12') %>
<%= title = "Medication/Allergies/History"
    start_table title %>
  <tbody>
    <% medications = [] %>
    <% if @parser.has_content('E12_14_0') %>
      <% @parser.parse_cluster('E12_14_0').each do |e12_14| %>
        <% medications << e12_14.concat('E12_14', 'E12_15', 'E12_16', 'E12_21') %>
      <% end %>
    <% end %>
    <%= start_row %>
      <%= labeled_cell 'Medications', medications.join('; ') %>
    </tr>

    <%= row({"Allergies" => "concat_with_comma('E12_22', 'E12_23')"}) %>
    <%= row({"History"   => "concat_with_comma('E12_24')"}) %>
  </tbody>
</table>
<br>
<% end %>

<% if @parser.has_content('E14') %>
    <%= title = "Vital Signs"
    start_table title, 16 %>

    <%= start_row %>
      <%= label_top "Time" %>
      <%= label_top "AVPU" %>
      <%= label_top "Side" %>
      <%= label_top "POS" %>

      <%= label_top "BP" %>
      <%= label_top "Pulse" %>
      <%= label_top "RR" %>
      <%= label_top "SPO2" %>

      <%= label_top "ETCO2" %>
      <%= label_top "CO" %>
      <%= label_top "BG" %>
      <%= label_top "T &deg;C" %>

      <%= label_top "Pain" %>
      <%= label_top "GCS" %>
      <%= label_top "RTS" %>
      <%= label_top "<b>PTS</b>" %>
    </tr>

    <% @parser.parse_cluster('E14').sort_by { |e14| Time.parse(e14.E14_01) rescue Time.now }.each do |e14| %>
        <%= start_row %>
          <%= cell (e14.parse_time('E14_01')) %>
          <%= cell (e14.E14_22) %>
          <%= cell (e14.E14_29) %>
          <%= cell (e14.E14_30) %>

          <%= cell ("#{[e14.E14_04, e14.E14_05].select {|v| !v.to_s.eql?('')}.compact.join('/')} #{e14.E14_06}") %>
          <%= cell ([(!e14.E14_07.to_s.eql?('') ? e14.E14_07 : e14.E14_08), e14.E14_10].compact.join(' ')) %>
          <%= cell ((e14.concat 'E14_11', 'E14_12')) %>
          <%= cell (e14.E14_09) %>

          <%= cell (e14.E14_13) %>
          <%= cell (e14.E14_31) %>
          <%= cell (e14.E14_14) %>
          <%= cell ((e14.concat 'E14_20', 'E14_21')) %>

          <%= cell (e14.E14_23) %>
          <%= cell ("#{e14.E14_19}/#{e14.E14_18}") %>
          <%= cell (e14.E14_27) %>
          <%= cell (e14.E14_28) %>
        </tr>
    <% end %>
    </tbody>
  </table>
<br>
<% end %>

<% if @parser.has_content('E14_01','E14_03','E14_33') %>
<%= title = "ECG"
    start_table title, 3 %>
  <%= start_row %>
    <%= label "Time" %>
    <%= label "3-Lead ECG" %>
    <%= label "12-Lead ECG" %>
  </tr>

  <% @parser.parse_cluster('E14').sort_by { |e14| Time.parse(e14.E14_01) rescue Time.now }.each do |e14| %>
    <% unless e14.E14_03.empty? and e14.E14_33.empty? %>
      <%= start_row %>
        <%= cell (e14.parse_time('E14_01')) %>
        <%= cell (e14.E14_34) %>
        <%= cell ("#{e14.E14_32.eql?('Yes') ? 'MI Suspected; ' : ''} #{e14.E14_33}") %>
      </tr>
    <% end %>
  <% end %>
</tbody>
</table>
<br>
<% end %>

<% if @parser.has_content('E18') || @parser.has_content('E19') %>
    <% rows = {:flow_chart => [], :pta => []} %>

    <% @parser.parse_cluster('E09').each do |e09| %>
      <% rows[:pta] << {:time => nil, 
                        :html => row_from_values(e09.E09_01,
                                                 e09.E09_02,
                                                 e09.E09_03,
                                                 e09.concat_with_comma('E09_17', 'E09_18', 'E09_19', 'E09_20'))} %>
    <% end %>

    <% @parser.parse_cluster('E18').each do |e18| %>
      <% if e18.E18_02.eql?('Yes') %>
        <% rows[:pta] << {:time => e18.E18_01, 
                          :html => row_from_values(e18.E18_12,
                                                   # e18.concat_with_comma('E18_13', 'E18_14', 'E18_15'),
                                                   e18.E18_15,
                                                   e18.E18_07,
                                                   e18.E18_16)} %>
      <% else %>
        <% rows[:flow_chart] << {:time => e18.E18_01, 
                                 :html => row_from_values(to_time(e18.E18_01), 
                                                          e18.E18_12,
                                                          e18.E18_16,
                                                          e18.concat_with_comma('E18_13', 'E18_14'))} %>
      <% end %>
    <% end %>

    <% @parser.parse_cluster('E19_01_0').each do |e19| %>
      <% if e19.E19_02.eql?('Yes') %>
        <% rows[:pta] << {:time => e19.E19_01, 
                          :html => row_from_values(e19.E19_15,
                                                   e19.E19_18,
                                                   e19.E19_08,
                                                   e19.concat_with_comma('E19_19', 'E19_20'))} %>
      <% else %>
        <% rows[:flow_chart] << {:time => e19.E19_01, 
                                 :html => row_from_values(to_time(e19.E19_01), 
                                                          e19.E19_15,
                                                          e19.E19_20,
                                                          e19.concat_with_comma('E19_16', 'E19_17'))} %>
      <% end %>
    <% end %>

    <%= title = "Flow Chart"
        start_table title, 4 %>
    <%= heading_row 'E18_01', 'E18_03', 'Description', 'E18_09' %>
    <% rows[:flow_chart].sort_by { |r| Time.parse(r[:time]) rescue Time.now }.each do |row| %>
      <%= row[:html] %>
    <% end %>
    </tbody>
  </table>
  <br>

  <% if !rows[:pta].empty? %>
      <%= title = "Treatments Prior to Arrival"
          start_table title, 4 %>
      <%= heading_row 'Treatment', 'Treatment By', 'Patient Response', 'Comments' %>
      <% rows[:pta].sort_by { |r| Time.parse(r[:time]) rescue Time.now }.each do |row| %>
        <%= row[:html] %>
      <% end %>
      </tbody>
    </table>
    <br>
  <% end %>
<% end %>

<% if @parser.has_content('E16') && @parser.has_content('E16_00_0') %>
    <% assessments = @parser.parse_assessments.reject {|a| a.parse_time('E16_03').nil?}
       n = 0
       total = assessments.size %>
    <% assessments.each do |assessment| %>
        <%= next if assessment.parse_time('E16_03').nil?
            n += 1
            title = "#{n==1 ? 'Initial' : 'Ongoing'} Assessment #{total > 1 ? "(#{n} of #{total})" : ''}"
            start_table title, 4 %>
                <tbody>
                    <%= assessment_table(assessment) %>
                </tbody>
            </table>
        <br>
    <% end %>
<% end %>

<% unless @parser.E13_01.empty? %>
  <%= title = "Narrative"
      start_table title %>
    <%= start_row %>
      <%= cell @parser.E13_01.to_br %>
