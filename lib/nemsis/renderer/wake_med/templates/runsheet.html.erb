<HEAD>
  <STYLE type="text/css">
      <!--

          /*p, h4, table {font-family: "Lucida Grande", verdana, arial, helvetica, sans-serif;}*/
          /*p, h4, table {font-family: Tahoma, Geneva, sans-serif}*/
      p, h4, table {
          font-family: "Trebuchet MS", Helvetica, sans-serif
      }

      p {
          font-size: small;
      }

      H4 {
          color: black
      }

          /* Each section can be a table */
      table, tbody, tr {
          border: 2px;
          border-style: solid;
          border-color: black;
          width: 700;
          color: black;
          margin: 0;
          padding: 0px;
      }

          /* The section can have a bold heading row */
      th {
          color: yellow;
          background-color: #6495ed;
          font-size: medium;
      }

          /* Default table cell for values */
      td {
          border: 1px;
          border-style: groove;
          border-color: black;
          font-size: small;
      }

          /* Default label cell */
      td.label {
          text-align: left;
          font-weight: bold;
          background-color: #dcdcdc;
      }

          /* Label on top of column */
      td.label-top {
          text-align: center;
          font-weight: bold;
          background-color: #dcdcdc;
      }

          /* Narrow label cell */
      td.label-narrow {
          text-align: left;
          font-weight: bold;
          background-color: #dcdcdc;
          width: 10%;
      }

          /* Medium label cell */
      td.label-medium {
          text-align: left;
          font-weight: bold;
          background-color: #dcdcdc;
          width: 25%;
      }

      -->
  </STYLE>
</HEAD>

