        <%= cell ([(e14.E14_07 || e14.E14_08), e14.E14_10].compact.join(' ')) %>
        <%= cell ((e14.concat 'E14_11', 'E14_12')) %>
        <%= cell (e14.E14_09) %>
        <%= cell (e14.E14_13) %>
        <%= cell (e14.E14_31) %>
        <%= cell (e14.E14_14) %>
        <%= cell ((e14.concat 'E14_20', 'E14_21')) %>
        <%= cell (e14.E14_23) %>
        <%= cell (e14.E14_19) %>
        <%= cell (e14.E14_27) %>
        <%= cell (e14.E14_28) %>
      </tr>
  <% end %>
</tbody>

</table>
<br>

<%= title = "ECG"
    start_table title, 3 %>
  <%= start_row %>
    <%= label "Time" %>
    <%= label "3-Lead ECG" %>
    <%= label "12-Lead ECG" %>
  </tr>

  <% @parser.parse_cluster('E14').sort_by { |e14| Time.parse(e14.E14_01) rescue Time.now }.each do |e14| %>
      <%= start_row %>
        <%= cell (e14.parse_time('E14_01')) %>
        <%= cell (e14.E14_03) %>
