  </tr>
  </tbody>
</table>
<br>
<% if @parser.has_content('E12') %>
<%= title = "Medication/Allergies/History"
    start_table title %>
  <tbody>
    <%= row( {"Medications" => "concat('E12_14', 'E12_15', 'E12_16')"}) %>
    <%= row( {"Allergies" => "concat('E12_08', 'E12_09')"}) %>
    <%= row( 'E12_10') %>
  </tbody>
</table>
<br>
<% end %>

<%= title = "Vital Signs"
    start_table title, 16 %>

  <%= start_row %>
    <%= label_top "Time" %>
    <%= label_top "AVPU" %>
    <%= label_top "Side" %>
    <%= label_top "POS" %>
    <%= label_top "BP" %>
    <%= label_top "Pulse" %>
    <%= label_top "RR" %>
    <%= label_top "SPO2" %>
    <%= label_top "ETCO2" %>
    <%= label_top "CO" %>
    <%= label_top "BG" %>
    <%= label_top "Temp" %>
    <%= label_top "Pain" %>
    <%= label_top "GCS" %>
    <%= label_top "RTS" %>
